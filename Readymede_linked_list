import java.util.LinkedList;

public class LinkedListProblem {
    public static void main(String[] args) {

        LinkedList<Integer> list = new LinkedList<>();

        list.add(10);
        list.add(20);
        list.add(30);

        list.addFirst(5);

        list.remove(Integer.valueOf(20));

        System.out.println(list);
    }
}
